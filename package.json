{
  "name": "cra2cra",
  "version": "1.0.0",
  "scripts": {
    "install": "npm-run-all --parallel install:app1 install:app2",
    "install:app1": "cd app1; npm i",
    "install:app2": "cd app2; npm i",
    "start": "npm-run-all --parallel start:app1 start:app2",
    "start:app1": "cd app1; npm start",
    "start:app2": "cd app2; npm start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}